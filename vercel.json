{
  "version": 2,
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/html/digitalize/assets/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "source": "/(.*)",
      "destination": "/html/digitalize/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
