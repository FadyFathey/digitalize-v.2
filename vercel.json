{
  "version": 2,
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/html/digitalize/assets/$1"
    },
    {
      "source": "/about",
      "destination": "/html/digitalize/index.html"
    },
    {
      "source": "/contact",
      "destination": "/html/digitalize/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/html/digitalize/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
